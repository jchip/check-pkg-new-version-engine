{
  "name": "check-pkg-new-version-engine",
  "version": "1.0.1",
  "description": "Provide generic engine for npm CLI packages to check newer version",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "xrun xarc/test-only",
    "prepublishOnly": "xrun [[build, docs], xarc/check]",
    "docs": "xrun xarc/docs",
    "coverage": "xrun xarc/test-cov"
  },
  "keywords": [
    "npm",
    "package",
    "new",
    "version",
    "check",
    "checker",
    "update",
    "notify",
    "notifier",
    "module",
    "cli"
  ],
  "author": "Joel Chen",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jchip/check-pkg-new-version-engine.git"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "@xarc/module-dev": "^3.2.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.16.0",
    "eslint-config-walmart": "^2.2.1",
    "eslint-plugin-filenames": "^1.1.0",
    "eslint-plugin-jsdoc": "^30.7.9",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.13",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "ci-info": "^2.0.0",
    "is-installed-globally": "^0.4.0",
    "mkdirp": "^1.0.4",
    "tslib": "^2.1.0"
  },
  "files": [
    "src",
    "dist"
  ],
  "prettier": {
    "printWidth": 100
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "@xarc/module-dev": {
    "features": [
      "eslint",
      "eslintTS",
      "jest",
      "jestTS",
      "prettier",
      "typedoc",
      "typescript"
    ]
  }
}
